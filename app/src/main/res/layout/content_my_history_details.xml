            <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardBackgroundColor="@color/white"
                card_view:cardElevation="3dp"
                card_view:cardPreventCornerOverlap="false"
                card_view:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="15dp">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/textView2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:text="Status"
                            android:textColor="@color/black"
                            android:textSize="18sp" />

                        <TextView
                            android:id="@+id/lbl_view_details"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="5dp"
                            android:background="?android:attr/selectableItemBackground"
                            android:drawablePadding="5dp"
                            android:drawableRight="@drawable/arrow_right_black_24dp"
                            android:gravity="center"
                            android:text="View Details"
                            android:textColor="@color/black"
                            android:textStyle="italic" />

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center_horizontal"
                        android:weightSum="6">

                        <RelativeLayout
                            android:id="@+id/layout_staus1"
                            android:layout_width="0dp"
                            android:layout_height="120dp"
                            android:layout_weight="1"
                            android:background="?android:attr/selectableItemBackground">

                            <ImageView
                                android:id="@+id/circle_status1"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_above="@+id/vertical_bar1"
                                android:layout_centerHorizontal="true"
                                android:src="@drawable/accept_circle" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@+id/circle_status1"
                                android:background="@color/dividerColor"></View>

                            <View
                                android:id="@+id/vertical_bar1"
                                android:layout_width="1dp"
                                android:layout_height="25dp"
                                android:layout_above="@+id/lblb_status1"
                                android:layout_centerHorizontal="true"
                                android:background="@color/dividerColor"></View>

                            <TextView
                                android:id="@+id/lblb_status1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:layout_centerHorizontal="true"
                                android:lines="2"
                                android:text="Deal Done\n"
                                android:textColor="@color/black"
                                android:textSize="10sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/layout_staus2"
                            android:layout_width="0dp"
                            android:layout_height="120dp"
                            android:layout_weight="1"
                            android:background="?android:attr/selectableItemBackground">

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_centerVertical="true"
                                android:layout_toLeftOf="@+id/circle_status2"
                                android:background="@color/dividerColor"></View>

                            <ImageView
                                android:id="@+id/circle_status2"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_below="@+id/vertical_bar2"
                                android:layout_centerHorizontal="true"
                                android:src="@drawable/accept_circle_gray" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@+id/circle_status2"
                                android:background="@color/dividerColor"></View>

                            <View
                                android:id="@+id/vertical_bar2"
                                android:layout_width="1dp"
                                android:layout_height="25dp"
                                android:layout_below="@+id/lblb_status2"
                                android:layout_centerHorizontal="true"
                                android:background="@color/dividerColor"></View>

                            <TextView
                                android:id="@+id/lblb_status2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_centerHorizontal="true"
                                android:gravity="bottom"
                                android:lines="2"
                                android:text="\nGoods Lifted"
                                android:textColor="@color/black"
                                android:textSize="10sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/layout_staus3"
                            android:layout_width="0dp"
                            android:layout_height="120dp"
                            android:layout_weight="1"
                            android:background="?android:attr/selectableItemBackground">

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_centerVertical="true"
                                android:layout_toLeftOf="@+id/circle_status3"
                                android:background="@color/dividerColor"></View>

                            <ImageView
                                android:id="@+id/circle_status3"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_above="@+id/vertical_bar3"
                                android:layout_centerHorizontal="true"
                                android:src="@drawable/accept_circle_gray" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@+id/circle_status3"
                                android:background="@color/dividerColor"></View>

                            <View
                                android:id="@+id/vertical_bar3"
                                android:layout_width="1dp"
                                android:layout_height="25dp"
                                android:layout_above="@+id/lblb_status3"
                                android:layout_centerHorizontal="true"
                                android:background="@color/dividerColor"></View>

                            <TextView
                                android:id="@+id/lblb_status3"
                                android:layout_width="wrap_content"
